# Uncomment this line to define a global platform for your project
### CAM - please provide a sensible value here, cocoapods defaults this to iOS 4.3 currently
platform :ios, '8.0'

target 'cam-example' do
  # Uncomment this line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!

  # Pods for cam-example

  pod 'NUUtil',:git => 'https://github.com/SciApps/util.git'
  pod 'curl',:git => 'https://github.com/SciApps/ios-curl.git'
  pod 'CURLConnection',:git => 'https://github.com/SciApps/CURLConnection.git'
  pod 'cam',:git => 'https://github.com/SciApps/cam.git'

end
